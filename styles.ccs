body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    background-color: #0d0d0d;
    color: #ffffff;
}
.container {
    text-align: center;
    padding: 50px;
}
h1 {
    font-size: 2.5em;
    color: #00ccff;
}
p {
    font-size: 1.2em;
}
nav ul {
    list-style: none;
    padding: 0;
}
nav ul li {
    display: inline;
    margin: 0 20px;
}
nav ul li a {
    color: #00ccff;
    text-decoration: none;
    font-size: 1.2em;
}
